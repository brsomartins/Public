@echo off

(
	echo **** Sytem date and time ******************************************************
	time /t
	date /t
	echo **** Sytem info ***************************************************************
	systeminfo
	echo **** User accounts ************************************************************
	net user
	echo **** Groups *******************************************************************
	net localgroup
	echo **** Network interfaces *******************************************************
	ipconfig /all
	echo **** Routing table ************************************************************
	route print
	echo **** ARP table ****************************************************************
	arp -a
	echo **** DNS cache ****************************************************************
	ipconfig /displaydns
	echo **** Network connections ******************************************************
	netstat -abn
) > sysinfo.txt